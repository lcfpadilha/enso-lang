struct Sentiment {
    mood: Enum<"Very Positive", "Positive", "Negative", "Very Negative", "Neutral">,
    score: Int
}

ai fn analyze(text: String) -> Sentiment {
    instruction: "Analyze the sentiment."
    model: "gemini-2.5-flash-lite"
}

print("Running Analysis...");
let result = analyze("it was good, but some of the things didn't work, but I recommend!");

print(result.value.mood);
print(result.value.score);